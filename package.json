{
  "name": "@winston-transports/firestore",
  "version": "1.0.5",
  "description": "transport for [winston logger]",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint src/**/*.ts --fix",
    "lint:prettier": "prettier --config .prettierrc --write src/**/*.ts",
    "publish:patch": "git pull && npm run build && npm version patch && npm publish && git push",
    "publish:minor": "git pull && npm run build && npm version minor && npm publish && git push",
    "publish:major": "git pull && npm run build && npm version major && npm publish && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AhmedEid3/winston-firestore.git"
  },
  "keywords": [
    "@winston-transports/firestore",
    "winston",
    "logger",
    "logging",
    "logs",
    "sysadmin",
    "bunyan",
    "pino",
    "loglevel",
    "tools",
    "jsonstream",
    "firestore",
    "winston-firestore"
  ],
  "author": "Ahmed Eid (https://github.com/AhmedEid3)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AhmedEid3/winston-firestore/issues"
  },
  "homepage": "https://github.com/AhmedEid3/winston-firestore#readme",
  "devDependencies": {
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@google-cloud/firestore": "^6.4.1",
    "winston-transport": "^4.5.0"
  }
}
